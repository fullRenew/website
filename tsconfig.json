{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@/components/*": [
        "src/components/*"
      ],
      "@/icons/*": [
        "src/icons/*"
      ],
      "@/lib/*": [
        "src/lib/*"
      ],
      "@/pages/*": [
        "src/pages/*"
      ],
      "@/layouts/*": [
        "src/layouts/*"
      ],
      "@/sections/*": [
        "src/sections/*"
      ],
      "@/db/*": [
        "src/db/*"
      ],
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  },
  "lib": ["dom", "es2016"],
}